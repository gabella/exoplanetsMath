Cell[BoxData[{
 RowBox[{
  RowBox[{"Clear", "[", "ggRed", "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"ggRed", "[", 
     RowBox[{"n_", ",", "e_"}], "]"}], ":=", 
    RowBox[{
     FractionBox[
      SuperscriptBox["n", "2"], 
      RowBox[{"6", 
       SuperscriptBox["e", "4"]}]], 
     RowBox[{"(", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"3", " ", 
        SuperscriptBox["e", "2"], " ", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"-", "1"}], "+", 
          SuperscriptBox["e", "2"]}], ")"}], " ", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"-", "1"}], "+", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{"-", "1"}], "+", 
             SuperscriptBox["e", "2"]}], ")"}], " ", 
           SuperscriptBox["n", "2"]}]}], ")"}], " ", 
        SuperscriptBox[
         RowBox[{"BesselJ", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"-", "1"}], "+", "n"}], ",", 
           RowBox[{"e", " ", "n"}]}], "]"}], "2"]}], "-", 
       RowBox[{"3", " ", "e", " ", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"-", "1"}], "+", 
          SuperscriptBox["e", "2"]}], ")"}], " ", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"-", "2"}], "+", 
          RowBox[{"n", " ", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{
              RowBox[{"-", "2"}], " ", 
              RowBox[{"(", 
               RowBox[{"2", "+", "n"}], ")"}]}], "+", 
             RowBox[{
              SuperscriptBox["e", "2"], " ", 
              RowBox[{"(", 
               RowBox[{"3", "+", 
                RowBox[{"2", " ", "n"}]}], ")"}]}]}], ")"}]}]}], ")"}], " ", 
        RowBox[{"BesselJ", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"-", "1"}], "+", "n"}], ",", 
          RowBox[{"e", " ", "n"}]}], "]"}], " ", 
        RowBox[{"BesselJ", "[", 
         RowBox[{"n", ",", 
          RowBox[{"e", " ", "n"}]}], "]"}]}], "+", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{
           RowBox[{"-", "3"}], " ", 
           SuperscriptBox["e", "6"], " ", 
           SuperscriptBox["n", "2"]}], "+", 
          RowBox[{"6", " ", 
           SuperscriptBox[
            RowBox[{"(", 
             RowBox[{"1", "+", "n"}], ")"}], "2"]}], "-", 
          RowBox[{"3", " ", 
           SuperscriptBox["e", "2"], " ", 
           RowBox[{"(", 
            RowBox[{"1", "+", "n"}], ")"}], " ", 
           RowBox[{"(", 
            RowBox[{"2", "+", 
             RowBox[{"5", " ", "n"}]}], ")"}]}], "+", 
          RowBox[{
           SuperscriptBox["e", "4"], " ", 
           RowBox[{"(", 
            RowBox[{"1", "+", 
             RowBox[{"3", " ", "n", " ", 
              RowBox[{"(", 
               RowBox[{"3", "+", 
                RowBox[{"4", " ", "n"}]}], ")"}]}]}], ")"}]}]}], ")"}], " ", 
        SuperscriptBox[
         RowBox[{"BesselJ", "[", 
          RowBox[{"n", ",", 
           RowBox[{"e", " ", "n"}]}], "]"}], "2"]}]}], "   ", ")"}]}]}], 
   ";"}], " "}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"ggRed", "::", "usage"}], " ", "=", " ", 
   "\"\<A simpler form for the g(n,e) funtion than seen in Peters-Matthews, \
Amarao-Seoane et al., or Maggiore.\>\""}], ";"}]}], "Input",
 InitializationCell->True,
 CellChangeTimes->{{3.725902143614675*^9, 3.725902146386407*^9}, {
  3.72590221344576*^9, 3.725902217173905*^9}, {3.7259045182341747`*^9, 
