Cell[BoxData[{
 RowBox[{
  RowBox[{"Clear", "[", 
   RowBox[{"freq", ",", " ", "powerCoeff"}], "]"}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"freq", "[", 
    RowBox[{"m1_", ",", " ", "m2_", ",", " ", "a_"}], "]"}], ":=", 
   RowBox[{
    FractionBox["1", 
     RowBox[{"2", "\[Pi]"}]], " ", 
    SqrtBox[
     RowBox[{" ", 
      FractionBox[
       RowBox[{"bigG", 
        RowBox[{"(", 
         RowBox[{"m1", "+", "m2"}], ")"}]}], 
       SuperscriptBox["a", "3"]]}]]}]}], " ", ";", 
  RowBox[{"(*", " ", 
   RowBox[{"Hz", ",", " ", 
    RowBox[{"orbital", " ", "period"}], ",", " ", 
    RowBox[{
     RowBox[{"masses", " ", "and", " ", "semi"}], "-", 
     RowBox[{"major", " ", 
      RowBox[{"axis", " ", ".", " ", 
       RowBox[{"!!", 
        RowBox[{"!", " ", 
         RowBox[{
         "Did", " ", "I", " ", "have", " ", "the", " ", "2", " ", "Pi", " ", 
          "in", " ", "the", " ", "wrong", " ", "place", 
          RowBox[{"??", "  ", 
           RowBox[{
           "Just", " ", "moved", " ", "if", " ", "from", " ", "the", " ", 
            "numerator", " ", "to", " ", "the", " ", "denominator", " ", 
            RowBox[{"20180125", " ", ".", "  ", "Do"}], " ", "I", " ", "use", 
            " ", "this", 
            RowBox[{"??", "  ", 
             RowBox[{"No", " ", "I", " ", "do", " ", 
              RowBox[{"not", "!!"}]}]}]}]}]}]}]}]}]}]}]}], "*)"}], "  ", 
  RowBox[{
   RowBox[{"freq", "::", "usage"}], " ", "=", " ", 
   "\"\<The frequency in Hz of the binary given the two masses m1, m2, and \
the semi-major axis a.\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"powerCoeff", "[", 
    RowBox[{"m1_", ",", "m2_", ",", "a_"}], "]"}], ":=", 
   RowBox[{"32", " ", 
    SuperscriptBox["bigG", "4"], " ", 
    FractionBox[
     RowBox[{
      SuperscriptBox["m1", "2"], 
      SuperscriptBox["m2", "2"], 
      RowBox[{"(", 
       RowBox[{"m1", "+", "m2"}], ")"}]}], 
     RowBox[{"5", " ", 
      SuperscriptBox["cee", "5"], 
      SuperscriptBox["a", "5"]}]]}]}], " ", ";", 
  RowBox[{"(*", " ", 
   RowBox[{"W", ",", " ", 
    RowBox[{
     RowBox[{"power", " ", "radiated", " ", "into", " ", "n"}], "-", 
     RowBox[{"th", " ", "mode", " ", "if", " ", "times", " ", "g", 
      RowBox[{"(", 
       RowBox[{"n", ",", "e"}], ")"}]}]}], ",", " ", 
    RowBox[{"or", " ", "total", " ", "if", " ", "times", " ", "f", 
     RowBox[{"(", 
      RowBox[{"n", ",", "e"}], ")"}]}]}], " ", "*)"}], "  ", 
  RowBox[{
   RowBox[{"powerCoeff", "::", "usage"}], " ", "=", " ", 
   "\"\<The coefficient in front of the power formulae, especially Maggiore's \
Eqns. 4.107 and 4.74.\>\""}], ";"}]}], "Input",
