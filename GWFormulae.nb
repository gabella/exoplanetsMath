Cell[BoxData[{
 RowBox[{"Clear", "[", 
  RowBox[{
  "chirpM", ",", "redM", ",", " ", "totM", ",", " ", "fr", ",", " ", "hh"}], 
  "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"chirpM", "[", 
    RowBox[{"m1_", ",", "m2_"}], "]"}], ":=", 
   FractionBox[
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{"m1", " ", "m2"}], ")"}], 
     RowBox[{"3", "/", "5"}]], 
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{"m1", "+", "m2"}], ")"}], 
     RowBox[{"1", "/", "5"}]]]}], ";", " ", 
  RowBox[{
   RowBox[{"chirpM", "::", "usage"}], " ", "=", " ", 
   "\"\<The chirp mass, from Amaro-Seone et al. Eqn. 9. \>\""}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"redM", "[", 
    RowBox[{"m1_", ",", "m2_"}], "]"}], ":=", 
   FractionBox[
    RowBox[{"m1", " ", "m2"}], 
    RowBox[{"m1", "+", "m2"}]]}], ";", "  ", 
  RowBox[{
   RowBox[{"redM", "::", "usage"}], " ", "=", " ", 
   "\"\<The reduced mass for two masses, m1 and m2.  Useful in Keplerian \
calculations and GW.\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"totM", "[", 
    RowBox[{"m1_", ",", "m2_"}], "]"}], ":=", 
   RowBox[{"m1", "+", "m2"}]}], " ", ";", " ", 
  RowBox[{
   RowBox[{"totM", "::", "usage"}], " ", "=", " ", 
   "\"\<The total mass, the sum of m1 and m2.\>\""}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"fr", "[", 
    RowBox[{"m1_", ",", "m2_", ",", "a_"}], "]"}], ":=", 
   RowBox[{
    FractionBox["1", 
     RowBox[{"2", " ", "\[Pi]"}]], 
    SqrtBox[
     FractionBox[
      RowBox[{"bigG", 
       RowBox[{"(", 
        RowBox[{"m1", "+", "m2"}], ")"}]}], 
      SuperscriptBox["a", "3"]]]}]}], ";", "  ", 
  RowBox[{
   RowBox[{"fr", "::", "usage"}], " ", "=", " ", 
   "\"\<The frequency of the orbit in Hz given the two masses m1, m2, and the \
semimajor axis a.\>\""}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"hh", " ", "[", 
    RowBox[{
    "n_", ",", "e_", ",", "m1_", ",", "m2_", ",", "a_", ",", " ", "dL_"}], 
    "]"}], ":=", " ", 
   RowBox[{
    FractionBox[
     SuperscriptBox["bigG", 
      RowBox[{"5", "/", "3"}]], 
     SuperscriptBox["cee", "4"]], "2", " ", 
    SqrtBox[
     FractionBox["32", "5"]], 
    FractionBox[
     SuperscriptBox[
      RowBox[{"chirpM", "[", 
       RowBox[{"m1", ",", "m2"}], "]"}], 
      RowBox[{"5", "/", "3"}]], 
     RowBox[{"n", " ", "dL"}]], 
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{"2", "\[Pi]", " ", 
       RowBox[{"fr", "[", 
        RowBox[{"m1", ",", "m2", ",", "a"}], "]"}]}], ")"}], 
     RowBox[{"2", "/", "3"}]], " ", 
    SqrtBox[
     RowBox[{"gg", "[", 
      RowBox[{"n", ",", "e"}], "]"}]]}]}], "  ", ";", 
  RowBox[{"(*", " ", 
   RowBox[{
    RowBox[{
     RowBox[{
      SubscriptBox["d", "L"], " ", "is", " ", "luminosity", " ", "distance", 
      " ", "d"}], "=", 
     RowBox[{
      SubscriptBox["d", "L"], "/", 
      RowBox[{"(", 
       RowBox[{"1", "+", "z"}], ")"}]}]}], ",", " ", 
    RowBox[{"z", "=", 
     RowBox[{"0", " ", "for", " ", "us"}]}], ",", " ", 
    RowBox[{
     SubscriptBox["f", "r"], " ", "is", " ", "orbital", " ", "freq"}], ",", 
    " ", 
    RowBox[{
     RowBox[{"gg", "[", "]"}], " ", "for", " ", "AS", " ", "and", " ", "for", 
     " ", "PM", " ", "and", " ", "for", " ", "Maggiore", " ", "and", " ", 
     "for", " ", "our", " ", "gred", " ", "are", " ", "all", " ", "the", " ", 
     "same"}], ",", " ", 
    RowBox[{"after", " ", "a", " ", "ton", " ", "of", " ", "work"}], ",", " ", 
    RowBox[{
    "tables", " ", "and", " ", "plots", " ", "of", " ", "differences", " ", 
     "show", " ", "it", " ", "to", " ", "be", " ", "true", " ", "to", " ", 
     "machine", " ", 
     RowBox[{"precision", "."}]}]}], " ", "*)"}], " ", 
  RowBox[{
   RowBox[{"hh", "::", "usage"}], " ", "=", " ", 
