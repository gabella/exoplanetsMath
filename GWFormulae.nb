          RowBox[{"retList", ",", " ", 
           RowBox[{"aa", "[", 
            RowBox[{"[", "i", "]"}], "]"}]}], " ", "]"}], ",", " ", 
         RowBox[{"AppendTo", "[", 
          RowBox[{"blankList", ",", " ", 
           RowBox[{"aa", "[", 
            RowBox[{"[", "i", "]"}], "]"}]}], " ", "]"}]}], "]"}]}], 
      "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"{", 
