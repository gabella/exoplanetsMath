Cell[BoxData[{
 RowBox[{
  RowBox[{"Clear", "[", 
   RowBox[{"bigA", ",", " ", "bigB", ",", " ", "bigC", ",", " ", "gg"}], 
   "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"bigA", "[", 
    RowBox[{"n_", ",", "e_", ",", " ", "asmaj_"}], "]"}], ":=", 
   RowBox[{
    FractionBox[
     SuperscriptBox["asmaj", "2"], "n"], 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"jj", "[", 
       RowBox[{
        RowBox[{"n", "-", "2"}], ",", 
        RowBox[{"n", " ", "e"}]}], "]"}], "-", 
      RowBox[{"jj", "[", 
       RowBox[{
        RowBox[{"n", "+", "2"}], ",", 
        RowBox[{"n", " ", "e"}]}], "]"}], "-", 
      RowBox[{"2", " ", "e", " ", 
       RowBox[{"jj", "[", 
        RowBox[{
         RowBox[{"n", "-", "1"}], ",", 
         RowBox[{"n", " ", "e"}]}], "]"}]}], "+", 
      RowBox[{"2", " ", "e", " ", 
       RowBox[{"jj", "[", 
        RowBox[{
         RowBox[{"n", "+", "1"}], ",", 
         RowBox[{"n", " ", "e"}]}], "]"}]}]}], " ", ")"}]}]}], ";", "   ", 
  RowBox[{
   RowBox[{"bigA", "::", "usage"}], " ", "=", 
   "\"\<The combination of Bessel functions called A from Maggioe Eqn. \
4.99.\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"bigB", "[", 
    RowBox[{"n_", ",", "e_", ",", " ", "asmaj_"}], "]"}], ":=", 
   RowBox[{
    FractionBox[
     RowBox[{
      SuperscriptBox["asmaj", "2"], 
      RowBox[{"(", 
       RowBox[{"1", "-", 
        SuperscriptBox["e", "2"]}], ")"}]}], "n"], 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"jj", "[", 
       RowBox[{
        RowBox[{"n", "+", "2"}], ",", 
        RowBox[{"n", " ", "e"}]}], "]"}], "-", 
      RowBox[{"jj", "[", 
       RowBox[{
        RowBox[{"n", "-", "2"}], ",", 
        RowBox[{"n", " ", "e"}]}], "]"}]}], ")"}]}]}], ";", "   ", 
  RowBox[{
   RowBox[{"bigB", "::", "usage"}], " ", "=", " ", 
   "\"\<The combination of Bessel function called B from Maggiore Eqn. 4.100.\
\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"bigC", "[", 
    RowBox[{"n_", ",", "e_", ",", " ", "asmaj_"}], "]"}], ":=", 
   RowBox[{
    FractionBox[
     RowBox[{
      SuperscriptBox["asmaj", "2"], 
      SqrtBox[
       RowBox[{"1", "-", 
        SuperscriptBox["e", "2"]}]]}], "n"], 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"jj", "[", 
       RowBox[{
        RowBox[{"n", "+", "2"}], ",", 
        RowBox[{"n", " ", "e"}]}], "]"}], "+", 
      RowBox[{"jj", "[", 
       RowBox[{
        RowBox[{"n", "-", "2"}], ",", 
        RowBox[{"n", " ", "e"}]}], "]"}], "-", 
      RowBox[{"e", " ", 
       RowBox[{"jj", "[", 
        RowBox[{
         RowBox[{"n", "+", "1"}], ",", 
         RowBox[{"n", " ", "e"}]}], "]"}]}], "-", " ", 
      RowBox[{"e", " ", 
       RowBox[{"jj", "[", 
        RowBox[{
         RowBox[{"n", "-", "1"}], ",", 
         RowBox[{"n", " ", "e"}]}], "]"}]}]}], " ", ")"}]}]}], ";", "    ", 
  RowBox[{
   RowBox[{"bigC", "::", "usage"}], " ", "=", " ", 
   "\"\<The combination of Bessel function called C from Maggiore Eqn. 4.100.\
\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"gg", "[", 
    RowBox[{"n_", ",", "e_"}], "]"}], ":=", 
   RowBox[{
    RowBox[{
     FractionBox[
      SuperscriptBox["n", "6"], 
      RowBox[{"96", " ", 
       SuperscriptBox["asmaj", "4"]}]], 
     RowBox[{"(", 
      RowBox[{
       SuperscriptBox[
        RowBox[{"bigA", "[", 
         RowBox[{"n", ",", "e", ",", "asmaj"}], "]"}], "2"], "+", 
       SuperscriptBox[
        RowBox[{"bigB", "[", 
         RowBox[{"n", ",", "e", ",", "asmaj"}], "]"}], "2"], "+", 
       RowBox[{"3", 
        SuperscriptBox[
         RowBox[{"bigC", "[", 
          RowBox[{"n", ",", "e", ",", "asmaj"}], "]"}], "2"]}], "-", 
       RowBox[{
        RowBox[{"bigA", "[", 
         RowBox[{"n", ",", "e", ",", "asmaj"}], "]"}], "*", 
        RowBox[{"bigB", "[", 
         RowBox[{"n", ",", "e", ",", "asmaj"}], "]"}]}]}], ")"}]}], "/.", 
    RowBox[{"asmaj", "\[Rule]", "1"}]}]}], ";", "  ", 
  RowBox[{"(*", " ", 
   RowBox[{
    RowBox[{"The", " ", 
     RowBox[{"asmaj", "'"}], "s", " ", "cancel", " ", "out"}], ",", " ", 
    RowBox[{
    "so", " ", "set", " ", "equal", " ", "to", " ", "1.", "  ", "Same", " ", 
     "power", " ", "of", " ", "asmaj", " ", "in", " ", 
     RowBox[{"bigA", "^", "2"}]}], ",", " ", 
    RowBox[{"bigB", "^", "2"}], ",", " ", 
    RowBox[{"bigC", "^", "2"}], ",", " ", 
    RowBox[{"and", " ", "bigA", "*", "bigB", " ", "as", " ", 
     RowBox[{"asmaj", "^", "4"}]}]}], " ", "*)"}], "  ", 
  RowBox[{
   RowBox[{"gg", "::", "usage"}], " ", "=", " ", 
   "\"\<The eccentricy and mode dependent power factor called g(n,e) in \
Maggiore's Eqn. 4.108, note the semi-major axis \\\"a\\\" cancels out of the \
numerator and denominator.\>\""}], ";"}]}], "Input",
 InitializationCell->True,
 CellChangeTimes->{{3.6709298861285973`*^9, 3.6709298968785667`*^9}, {
  3.6709307949869757`*^9, 3.670930927054208*^9}, {3.6709309634804688`*^9, 
