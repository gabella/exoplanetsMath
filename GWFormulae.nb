Cell[BoxData[{
 RowBox[{
  RowBox[{"Clear", "[", "findSecondRoot", "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"findSecondRoot", "[", 
    RowBox[{"ee_", ",", " ", "lim_", ",", " ", "gg_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"alist", ",", 
       RowBox[{"nmax", "=", "200"}], ",", 
       RowBox[{"iloop", "=", "True"}], ",", " ", 
       RowBox[{"ifirstRoot", "=", "False"}], ",", " ", "ii", ",", " ", 
       "prev"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"alist", "=", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"ii", ",", 
           RowBox[{
            RowBox[{
             RowBox[{"gg", "[", 
              RowBox[{"ii", ",", "ee"}], "]"}], "-", "lim"}], ">", "0"}]}], 
          " ", "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"ii", ",", " ", "1", ",", " ", "nmax"}], "}"}]}], "]"}]}], 
      ";", "  ", 
      RowBox[{"(*", " ", 
       RowBox[{
        RowBox[{"Check", " ", "for", " ", "neg"}], ",", " ", 
        RowBox[{"then", " ", "pos"}], ",", " ", 
        RowBox[{"then", " ", "neg"}], ",", " ", 
        RowBox[{"there", " ", "is", " ", "second", " ", 
         RowBox[{"root", "."}]}]}], " ", "*)"}], "\[IndentingNewLine]", 
      RowBox[{"ii", "=", "1"}], ";", "\[IndentingNewLine]", 
      RowBox[{"prev", "=", 
       RowBox[{"alist", "[", 
        RowBox[{"[", 
         RowBox[{"ii", ",", "2"}], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"While", "[", 
       RowBox[{"iloop", ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"If", "[", " ", 
          RowBox[{
           RowBox[{"Xor", "[", 
            RowBox[{"prev", ",", 
             RowBox[{"alist", "[", 
              RowBox[{"[", 
               RowBox[{"ii", ",", "2"}], "]"}], "]"}]}], "]"}], ",", 
           "\[IndentingNewLine]", 
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{"ifirstRoot", "\[Equal]", "False"}], ",", 
             RowBox[{"ifirstRoot", "=", "True"}], ",", " ", 
             RowBox[{"iloop", "=", "False"}]}], " ", "]"}], ","}], " ", "]"}],
          ";", 
         RowBox[{"(*", " ", 
          RowBox[{
           RowBox[{
           "True", " ", "when", " ", "goes", " ", "from", " ", "neg", " ", 
            "to", " ", "pos", " ", "in", " ", "above", " ", "ggPM"}], "-", 
           RowBox[{"lim", "."}]}], " ", "*)"}], "\[IndentingNewLine]", 
         RowBox[{"prev", " ", "=", " ", 
          RowBox[{"alist", "[", 
           RowBox[{"[", 
            RowBox[{"ii", ",", "2"}], "]"}], "]"}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"ii", "++"}], ";"}]}], "\[IndentingNewLine]", "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{"Now", " ", "ii", " ", "is", " ", "near", " ", "second", " ", 
        RowBox[{"root", "."}]}], " ", "*)"}], "\[IndentingNewLine]", 
      RowBox[{"FindRoot", "[", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"gg", "[", 
           RowBox[{"nn", ",", "ee"}], "]"}], "-", "lim"}], "\[Equal]", "0"}], 
        ",", 
        RowBox[{"{", 
         RowBox[{"nn", ",", "ii"}], "}"}]}], "]"}]}]}], "\[IndentingNewLine]",
     "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"findSecondRoot", "::", "usage"}], " ", "=", " ", 
   "\"\<Given the eccentriciy, some limit (nmax uses 0.05), and the gg \
function, find the point where the g(n,e) curve crosses limit, the second \
time, that is the high side.\>\""}], ";"}]}], "Input",
 InitializationCell->True,
 CellChangeTimes->{{3.671362278545355*^9, 3.671362554930307*^9}, {
